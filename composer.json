{
    "name": "serafim/curry",
    "license": "MIT",
    "type": "library",
    "description": "Convenient implementation of function currying and partial application",
    "keywords": ["currying", "partial application", "functional", "lambda", "closure", "sugar"],
    "support": {
        "issues": "https://github.com/SerafimArts/Curry/issues",
        "source": "https://github.com/SerafimArts/Curry"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\Curry\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "illuminate/support": "~5.5",
        "symfony/var-dumper": "~4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\Curry\\Tests\\": "tests/"
        }
    }
}
