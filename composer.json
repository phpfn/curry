{
    "name": "phpfn/curry",
    "license": "MIT",
    "type": "library",
    "description": "Convenient implementation of function currying and partial application",
    "keywords": ["currying", "partial application", "functional", "lambda", "closure", "sugar"],
    "homepage": "https://github.com/phpfn/curry",
    "support": {
        "issues": "https://github.com/phpfn/curry/issues",
        "source": "https://github.com/phpfn/curry"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Fun\\Curry\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Fun\\Curry\\Tests\\": "tests"
        }
    }
}
